<%- include('../partials/header') %>

<form id="new-form" action="/flights" method="POST">
  <label>Airline:</label>
  <input type="text" name="airline" ></input>
 
  <label>Flight Number:</label>
  <input type="number" name="flightNo" ></input>
  
  <label>Departure Date:</label>
  <input type="date" name="departs" ></input>
  
  <label>Airport:</label>
  <input type="text" name="airport" ></input>
  
  <label>Destination:</label>
  <input type="text" name="destination" ></input>

  <input type="submit" value="add flight">
  

</form>

